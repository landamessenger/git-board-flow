{
  "mcpServers": {
    "example-stdio": {
      "name": "example-stdio",
      "command": "npx",
      "args": ["@modelcontextprotocol/server-postgres"],
      "env": {
        "DATABASE_URL": "postgresql://user:password@localhost:5432/dbname"
      },
      "transport": "stdio"
    },
    "example-http": {
      "name": "example-http",
      "url": "https://mcp-server.example.com",
      "headers": {
        "Authorization": "Bearer your-token-here"
      },
      "transport": "http"
    }
  }
}

